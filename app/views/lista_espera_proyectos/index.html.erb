<p id="notice"><%= notice %></p>

<h1>Listing Lista Espera Proyectos</h1>

<table>
  <thead>
    <tr>
      <th>Proyecto</th>
      <th>Usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lista_espera_proyectos.each do |lista_espera_proyecto| %>
      <tr>
        <td><%= lista_espera_proyecto.proyecto_id %></td>
        <td><%= lista_espera_proyecto.usuario %></td>
        <td><%= link_to 'Show', lista_espera_proyecto %></td>
        <td><%= link_to 'Edit', edit_lista_espera_proyecto_path(lista_espera_proyecto) %></td>
        <td><%= link_to 'Destroy', lista_espera_proyecto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

