<p id="notice"><%= notice %></p>

<h1>Listing Proyecto Carreras</h1>

<table>
  <thead>
    <tr>
      <th>Proyecto</th>
      <th>Carrera</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proyecto_carreras.each do |proyecto_carrera| %>
      <tr>
        <td><%= proyecto_carrera.proyecto_id %></td>
        <td><%= proyecto_carrera.carrera_id %></td>
        <td><%= link_to 'Show', proyecto_carrera %></td>
        <td><%= link_to 'Edit', edit_proyecto_carrera_path(proyecto_carrera) %></td>
        <td><%= link_to 'Destroy', proyecto_carrera, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proyecto carrera', new_proyecto_carrera_path %>
